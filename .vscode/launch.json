{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload",
                "--port",
                "8001" // 백엔드 실행 시 사용하려는 포트로 변경하세요 (예: 8001)
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "GOOGLE_API_KEY" : "AIzaSyBu280VVK9YaAmEDLgCigcInHY_QnWgkEI"
            }
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:5173", // 1단계에서 확인한 주소
            "webRoot": "${workspaceFolder}/frontend/src" // 소스 코드 위치
        }
    ]
}